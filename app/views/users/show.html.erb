<div class="my_profile">
  <div class="upper">
    <%= @name %>
  </div>
  <div class="lower">
    <%= @posts.each do |p| %>
    <div class="col-md-6 mx-auto mt-5">
      <div class="card flex-md-row mb-4 shadow-sm h-md-250">
        <div class="card-body d-flex flex-column align-items-start p_height">
          <strong class="d-inline-block mb-2 text-success"><%=p.place%></strong>
          <h3 class="mb-0">
            <a class="text-dark p_title" href="#"><%= p.title %></a>
          </h3>
          <h3 class="mb-0">
            <a class="text-dark p_title" href="#"><%= p.content %></a>
          </h3>
          <%= link_to  "jump to page", post_path(@posts.ids), class: "jump_link" %>
        </div>
        <%= image_tag p.image, class: "card-img-right flex-auto d-none d-lg-block image_size" if p.image.attached?%>
      </div>
    </div>
    <%end%>
  </div>
</div>